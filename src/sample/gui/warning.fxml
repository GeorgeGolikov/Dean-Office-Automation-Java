<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="200.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.WarningController">
   <children>
      <ImageView fitHeight="111.0" fitWidth="112.0" layoutX="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@achtung.png" />
         </image>
      </ImageView>
      <Label fx:id="label" alignment="TOP_LEFT" layoutX="140.0" layoutY="14.0" prefHeight="111.0" prefWidth="189.0" text="Warning" />
      <Button fx:id="okButton" layoutX="14.0" layoutY="144.0" mnemonicParsing="false" onAction="#buttonOkClicked" text="Продолжить" />
      <Button fx:id="cancelButton" layoutX="230.0" layoutY="144.0" mnemonicParsing="false" onAction="#cancelButtonClicked" prefHeight="25.0" prefWidth="86.0" text="Отмена" />
   </children>
</AnchorPane>
